  {
    "@id": "https://spectart.com/temporary/8515b2ae-726e-4e0e-8485-2b1609a93f82",
    "@type": [
      "http://schema.org/Event"
    ],
    "http://schema.org/name": [
      {
        "@value": "Shaina Hayes"
