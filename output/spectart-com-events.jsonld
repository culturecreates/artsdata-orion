  {
    "@id": "https://spectart.com/temporary/ac734862-bd0d-4929-af38-7c015424249f",
    "@type": [
      "http://schema.org/Event"
    ],
    "http://schema.org/name": [
      {
        "@value": "Shaina Hayes"
