  {
    "@id": "https://spectart.com/temporary/d334b10c-beab-4633-9008-3213012a49ee",
    "@type": [
      "http://schema.org/Event"
    ],
    "http://schema.org/name": [
      {
        "@value": "Shaina Hayes"
