    "http://schema.org/name": [
      {
        "@language": "fr",
        "@value": "Jérôme Combier"
      }
    ],
    "http://schema.org/givenName": [
      {
        "@language": "fr",
        "@value": "Jérôme"
      }
    ],
    "http://schema.org/familyName": [
      {
        "@language": "fr",
        "@value": "Combier"
